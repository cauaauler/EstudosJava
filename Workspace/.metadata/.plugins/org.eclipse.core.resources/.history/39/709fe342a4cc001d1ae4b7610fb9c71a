package exemplos;

import java.util.Random;

public class Exemplo03 {
	
	public static void main(String[] args) {
		
		Random r = new Random();
		//System.out.println(r.nextInt(2));
		r.nextInt(2);
		
		int um = 0;
		int zero = 0;
		for(int i = 0;i<100000;i++) {
			r.nextInt(2);
			if(r == 0) {
				zero++;
			}else {
				um++;
			}
			
		}
		
	}

}
