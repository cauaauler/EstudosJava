package exercicios;

import java.util.Scanner;

public class Exerc10 {
	public static void main(String[] args) {
		
		Scanner leitor = new Scanner(System.in);
		
		
		System.out.print("Valor do salário mínimo: ");
		double salario_min = Double.parseDouble(leitor.nextLine());
		
		System.out.print("Valor do seu salário: ");
		double salario = leitor.nextInt();
		
		double conta = salario/salario_min;
		
		System.out.print("Você recebe "+conta+" salários mínimos");
	}

}