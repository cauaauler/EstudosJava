package desafios;

public class Desafio1 {
	public static void main(String[] args) {
		
		char[] letras = {'S', 'R', 'F', 'I'};
		
		for(int i =0;i<letras.length;i++) {
			for(int x = 0;x<=i;x++) {
				
				System.out.print(letras[(i-3)]);
				
				System.out.println("");
			}
			
			
			
		}
	}
}	
