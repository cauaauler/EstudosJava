package exemplos;

import java.util.Random;

public class Exemplo03 {
	
	public static void main(String[] args) {
		
		
		//System.out.println(r.nextInt(2));
		
		float um = 0;
		float zero = 0;
		for(int i = 0;i<100000;i++) {
			Random r = new Random();
			if(r == 0) {
				zero++;
			}else {
				um++;
			}
			
		}
		
	}

}
