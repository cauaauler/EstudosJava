package exemplos;

public class Exemplo1 {
	
	public static void main(String[] args) {
		
		String cidade = "Feliz";
		
		//System.out.println(cidade.charAt(0));
		
		for(int i = 0;i<cidade.length();i++) {
			System.out.print(cidade.charAt(i));
		}
		System.out.println();
		int n1 = 10;
		int n2 = 20;
		System.out.println("Soma: "+(n1 + n2));
	}

}
