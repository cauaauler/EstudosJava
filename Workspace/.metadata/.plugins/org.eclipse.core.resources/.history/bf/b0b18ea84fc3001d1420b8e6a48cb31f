package exercícios;

import java.util.Scanner;

public class Exer5 {

	public static void main(String[] args) {
		
		Scanner leitor = new Scanner(System.in);
		
		System.out.println("Digite um número: ");
		int n = Integer.parseInt(leitor.nextLine());
		
		int fatorial = n-1;
		
		for(int i=0;i<n;i++) {
			int conta = n*fatorial;
			fatorial--;
			
		}
		

	}

}
