package exer3_OO;

public class TestePerm {
	
	public static void main(String[] args) {
		
		Permutacao p = new Permutacao();
		
		p.n1 = "242";
		p.n2 = "224";
		
		p.n1 = p.n1.replace("0", "");
		p.n1 = p.n1.replace(" ", "");
		
		p.n2 = p.n2.replace("0", "");
		p.n2 = p.n2.replace(" ", "");
		
		p.ehMaior();
	
		p.ehPermutacao();
		
	}

}
