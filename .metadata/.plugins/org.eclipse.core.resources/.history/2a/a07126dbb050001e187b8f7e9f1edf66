package exer;

public class Exer01 {
	public static void main(String[] args) {
		String palavra = "Java";
		//char[] letras = palavra.toCharArray();
		
		
		for (int i = 0; i < palavra.length(); i++) {
			for (int j = 0; j <= i; j++) {
				System.out.println(palavra.charAt(j));
			}
			
		}
		
	}

}
